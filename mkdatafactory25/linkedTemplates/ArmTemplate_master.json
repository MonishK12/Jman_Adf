{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "mkdatafactory25"
		},
		"Ls_Ablb_Input_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Ls_Ablb_Input'"
		},
		"Ls_Ass_Store_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Ls_Ass_Store'"
		},
		"Ls_Ass_Store_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "datamk.database.windows.net"
		},
		"Ls_Ass_Store_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "Trial"
		},
		"Ls_Ass_Store_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "mk"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "mkdatafactory25_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Ls_Ablb_Input_connectionString": {
						"value": "[parameters('Ls_Ablb_Input_connectionString')]"
					},
					"Ls_Ass_Store_password": {
						"value": "[parameters('Ls_Ass_Store_password')]"
					},
					"Ls_Ass_Store_properties_typeProperties_server": {
						"value": "[parameters('Ls_Ass_Store_properties_typeProperties_server')]"
					},
					"Ls_Ass_Store_properties_typeProperties_database": {
						"value": "[parameters('Ls_Ass_Store_properties_typeProperties_database')]"
					},
					"Ls_Ass_Store_properties_typeProperties_userName": {
						"value": "[parameters('Ls_Ass_Store_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}